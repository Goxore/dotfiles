# main
Print
  maim -s --format=png /dev/stdout | xclip -selection clipboard -t image/png -i

super + d
  rofi -show drun -show-icons

super + shift + x 
  rofi -show power-menu -modi power-menu:~/.local/bin/rofi-power-menu

# scripts
super + a
	bash $SCRIPTS/fastaccess.sh

super + shift + e
	bash $SCRIPTS/editconfig.sh

super + s
	bash $SCRIPTS/sb -r

super + v
	bash $SCRIPTS/mictoggle.sh

super + shift + c
	rofi -modi 'clipboard:greenclip print' -show clipboard -run-command '{cmd}'

#music
super + plus
	mpc volume +5

super + equal
	mpc volume +5

super + minus
	mpc volume -5

super + p
	mpc toggle

super + bracketright
	mpc next

super + bracketleft
	mpc prev

super + u
	rofi-rbw
super + Delete
	$SCRIPTS/holdwork.sh

super + Insert
	$SCRIPTS/continuework.sh
