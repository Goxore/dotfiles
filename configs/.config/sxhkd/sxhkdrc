# main
Print
  maim -s --format=png /dev/stdout | xclip -selection clipboard -t image/png -i

super + d
  rofi -show drun -show-icons

super + shift + x 
  rofi -show power-menu -modi power-menu:/usr/bin/rofi-power-menu

# scripts
super + a
	bash $SCRIPTS/fastAccess/fastaccess.sh

super + e
    $TERMINAL -e $HOME/scripts/general/editconfig.sh
	# bash $SCRIPTS/editconfig.sh

super + v
	bash $SCRIPTS/mictoggle.sh

super + shift + c
	rofi -modi 'clipboard:greenclip print' -show clipboard -run-command '{cmd}'

super + shift + k
    $SCRIPTS/launcher/killprocess.sh

super + u
	rofi-rbw

super + Delete
	$SCRIPTS/holdwork.sh

super + Insert
	$SCRIPTS/continuework.sh

super + ctrl + e
  $SCRIPTS/emoji.sh

#music
super + plus
	mpc volume +5

super + equal
	mpc volume +5

super + minus
	mpc volume -5

super + p
	mpc toggle

super + bracketright
	mpc next

super + bracketleft
	mpc prev

XF86AudioPlay
    xdotool click 1
	#mpc toggle

XF86AudioNext
	mpc next

#Programs
super + alt + b
	$BROWSER

super + ctrl + b
	blueberry

super + alt + t
  telegram-desktop

super + alt + w
  $TERMINAL -e bash -c "sleep 0.1 && nvim $HOME/vimwiki/index.wiki"

super + alt + p
  bitwarden-desktop
